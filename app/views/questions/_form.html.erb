<div class="row">
  <div class="col-sm-2"></div>
  <div class="col-sm-8">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">
          New Question
        </h3>
      </div>
      <div class="panel-body">
        <%= bootstrap_form_for @question, remote: true do |f| %>
          <span id="new_question_errors"></span>
          <div class="form-group">
            <%= f.text_field :title %>
          </div>
          <div class="form-group">
            <%= f.text_area :body %>
          </div>
          <%= f.submit %>
        <% end %>
      </div>
    </div>
  </div>
  <div class="col-sm-2"></div>
</div>
